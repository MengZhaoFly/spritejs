       if(type === 'url' || type === 'drop-shadow' && i === 3) {\n          value = n;\n        } else {\n          value = toNumber(n);\n        }\n\n        if(/%$/.test(n)) {\n          value /= 100;\n        }\n        return value;\n      });\n      ret.push({type, args});\n    });\n  }\n\n  return ret;\n}\n\nexport function applyFilters(mesh, filters) {\n  mesh.clearFilter();\n  if(filters) {\n    filters.forEach(({type, args}) => {\n      let method = type;\n      if(method === 'drop-shadow') method = 'dropShadow';\n      else if(method === 'hue-rotate') method = 'hueRotate';\n      mesh[method](...args);\n    });\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"6674cbdb19b03a3b5d0f8785e4440faa","cacheData":{"env":{}}}